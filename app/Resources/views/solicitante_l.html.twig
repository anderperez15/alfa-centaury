      <div class="container-fluid">
      
               

 {% for flashMessage in app.session.flashbag.get('notice') %}
 
    <div class="modal fade in" id="modalmsg" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" style="display:block">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close closemodal" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">Mensaje</h4>
      </div>
      <div class="modal-body">
  
    <h4 style="font-weight: bold;">
        {{ flashMessage|raw }}
    </h4>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default closemodal" data-dismiss="modal">Cerrar</button>
      </div>
    </div>
  </div>
</div>
{% endfor %}

          {% for flash in app.session.flashbag.get('danger') %}

                  <div class="alert alert-danger fade in">
                      <button type="button" class="close" data-dismiss="alert">&times;</button>
                      {{ flash }}
                  </div>
              {% endfor %}
              
            {% for flash in app.session.flashbag.get('success') %}
                  <div class="alert alert-success fade in">
                      <button type="button" class="close" data-dismiss="alert">&times;</button>
                      {{ flash }}
                  </div>
              {% endfor %}
          
      {% block page %}

      {% endblock %}

      </div>

        <script src="{{ asset('bundles/helpdesk/js/jquery.min.js') }}"></script>
        <!-- Bootstrap Core JavaScript -->
        <script src="{{ asset('bundles/helpdesk/js/bootstrap.min.js') }}"></script>
        <script>
        $(document).ready(function(){
        $('body').on('click','.closemodal',function (){
        $('#modalmsg').css("display", "none");
        });
        });
        </script>


      {% block addScripts %} {% endblock %}
    </body>
</html>
